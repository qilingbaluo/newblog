rd.length<8 ) {\r\n                    alert('密码太短')\r\n                    return\r\n                }\r\n                //提交注册 dispatch调用store的index.js的axios的请求\r\n                this.$store.dispatch('blogRegister',this.formData)\r\n            }\r\n        },\r\n\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    #register-page {\r\n        height: 80vh;\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: center;\r\n    }\r\n\r\n    .loginbox {\r\n        padding: 10px 10px;\r\n    }\r\n</style>"],"sourceRoot":"src/views"}]}