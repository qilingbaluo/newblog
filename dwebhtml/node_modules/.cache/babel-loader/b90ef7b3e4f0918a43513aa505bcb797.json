AA;;AACA,UAAA,SAAA,GAAA,IAAA,CADA,CAEA;;AACA,MAAA,IAAA,CAAA,OAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,EAAA,GAAA,MAAA,CAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,EAAA;AACA;;AACA,YAAA,GAAA,CAAA,KAAA,IAAA,MAAA,CAAA,cAAA,EAAA;AACA,UAAA,KAAA,CAAA,MAAA,CAAA;AACA,UAAA,SAAA,GAAA,KAAA;AACA;AACA;;AACA,YAAA,GAAA,CAAA,QAAA,EAAA;AACA,cAAA,GAAA,CAAA,QAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,YAAA,MAAA,CAAA,aAAA,CAAA,GAAA,CAAA,QAAA;AACA;AACA;AAEA,OAfA;AAgBA,aAAA,SAAA;AACA,KApHA;AAsHA,IAAA,WAtHA,uBAsHA,IAtHA,EAsHA,KAtHA,EAsHA;AAAA;;AACA,MAAA,KAAA,CAAA;AACA,QAAA,MAAA,EAAA,KADA;AAEA,QAAA,GAAA,EAAA,2CAFA;AAGA,QAAA,MAAA,EAAA;AACA,UAAA,IAAA,EAAA,IADA;AAEA,UAAA,QAAA,EAAA,KAAA,QAFA;AAGA,UAAA,KAAA,EAAA;AAHA;AAHA,OAAA,CAAA,CAQA,IARA,CAQA,UAAA,GAAA,EAAA;AACA;AACA,QAAA,MAAA,CAAA,YAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;AAEA,OAbA;AAcA,KArIA;AAuIA,IAAA,aAvIA,yBAuIA,GAvIA,EAuIA;AACA;AACA,WAAA,WAAA,GAAA,GAAA;AACA,WAAA,WAAA,CAAA,GAAA;AACA,KA3IA;AA4IA,IAAA,MA5IA,kBA4IA,CA5IA,EA4IA,IA5IA,EA4IA;AAAA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA,CAAA,EAAA;AACA,MAAA,KAAA,CAAA;AACA,QAAA,MAAA,EAAA,QADA;AAEA,QAAA,OAAA,EAAA;AACA,0BAAA;AADA,SAFA;AAKA,QAAA,GAAA,EAAA,yCALA;AAMA,QAAA,IAAA,EAAA,EAAA,CAAA,SAAA,CAAA;AACA,UAAA,KAAA,EAAA,KAAA,MAAA,CAAA,OAAA,CAAA,cADA;AAEA,UAAA,EAAA,EAAA,IAAA,CAAA;AAFA,SAAA;AANA,OAAA,CAAA,CAUA,IAVA,CAUA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,IAAA;;AACA,YAAA,GAAA,CAAA,IAAA,IAAA,SAAA,EAAA;AACA,UAAA,KAAA,CAAA,MAAA,CAAA;AACA;AACA;;AACA,YAAA,GAAA,CAAA,IAAA,IAAA,QAAA,EAAA;AACA,UAAA,KAAA,CAAA,MAAA,CAAA;AACA;AACA;;AACA,YAAA,GAAA,CAAA,IAAA,IAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,YAAA;AACA;AACA,OAvBA;AAwBA,KAtKA;AAuKA,IAAA,aAvKA,yBAuKA,CAvKA,QAuKA;AAAA;;AAAA,UAAA,IAAA,QAAA,IAAA;AAAA,UAAA,IAAA,QAAA,IAAA;AACA;AAAA,iBACA;AADA,qBAGA,IAAA,CAAA,KAHA,oBAIA,IAAA,CAAA,IAAA,CAAA,WAJA;AAAA;AAAA,kBAMA,MANA;AAAA,kBAMA;AANA;AAAA,iBAMA,kBANA;AAAA;AAAA,mBAMA;AAAA,mBAAA,MAAA,CAAA,MAAA,CAAA,IAAA,EAAA,IAAA,CAAA;AAAA;AANA;AAAA;AASA;AAjLA;AA3BA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <BreadMenu :page_name=\"'栏目管理'\"></BreadMenu>\r\n\r\n        <!-- 内容 -->\r\n        <div class=\"body dweb\">\r\n            <el-row :gutter=\"10\">\r\n                <!-- 栏目树型图 -->\r\n                <el-col :span=\"6\">\r\n                    <div class=\"dweb\">\r\n                        <h5>栏目结构</h5>\r\n                        <el-divider></el-divider>\r\n                    </div>\r\n                    <div class=\"body dweb\" style=\"display: flex;\">\r\n                        <el-input v-model=\"new_lanmu_name\" placeholder=\"请输入新栏目名称\"></el-input>\r\n                        <el-button @click=\"pushLanmuList()\" type=\"danger\" plain>确定</el-button>\r\n                    </div>\r\n                    <div class=\"body dweb\">\r\n                        <el-tree :data=\"lanmu_tree\" node-key=\"id\" default-expand-all draggable\r\n                            :render-content=\"renderContent\" @node-click=\"choosed_lanmu_articleList\">\r\n                        </el-tree>\r\n                    </div>\r\n                    <div class=\"save-tree body dweb\" style=\"float: left;\">\r\n                        <el-button @click=\"getLanmuTree()\" type=\"danger\" size=\"mini\" plain>恢复结构</el-button>\r\n                        <el-button @click=\"saveLanmuTree()\" type=\"success\" size=\"mini\" plain>保存结构</el-button>\r\n                        <el-button type=\"primary\" size=\"mini\" plain>全部文章获取</el-button>\r\n                    </div>\r\n                </el-col>\r\n\r\n\r\n\r\n                <!-- 栏目列表 -->\r\n                <el-col :span=\"18\">\r\n                    <div v-if=\"choose_lanmu_name\" class=\"dweb\">\r\n                        <h5>栏目名称：</h5>\r\n                    </div>\r\n                    <div v-else class=\"dweb\">\r\n                    </div>\r\n                    <div class=\"dweb\">\r\n                            <h5>{{ choose_lanmu_name }}   </h5>\r\n                    </div>\r\n                    <el-divider></el-divider>\r\n                    <!-- 文章列表 -->\r\n                    <div class=\"dweb\" style=\"margin-top:5px;min-height: 468px;\">\r\n                        <el-row>\r\n                            <el-col v-for=\"item in article_list\" :key=\"item.id\" :span=\"24\">\r\n                                <div class=\"card dweb\">\r\n                                    <el-row>\r\n                                        <el-col :xs=\"24\" :lg=\"6\">\r\n                                            <el-image style=\"height: 60px\" :src=\"item.cover\" :fit=\"'cover'\">\r\n                                            </el-image>\r\n                                        </el-col>\r\n                                        <el-col class=\"text-item\" :xs=\"24\" :lg=\"4\">\r\n                                            <span>\r\n                                                {{item.title}}\r\n                                            </span>\r\n                                        </el-col>\r\n                                        <el-col class=\"text-item\" :xs=\"12\" :lg=\"7\">\r\n                                            <span>\r\n                                                发布者：{{item.nickName}}\r\n                                            </span>\r\n                                        </el-col>\r\n                                        <el-col class=\"text-item\" :xs=\"12\" :lg=\"7\">\r\n                                            <el-popover placement=\"right\" width=\"100\" trigger=\"click\">\r\n                                                <el-tree :data=\"lanmu_tree\" node-key=\"id\" default-expand-all\r\n                                                    @node-click=\"choosed_lanmu\">\r\n                                                </el-tree>\r\n                                                <el-button @click=\"saveArticleToLanmu(item.id)\" size=\"mini\"\r\n                                                    type=\"warning\">确定\r\n                                                </el-button>\r\n                                                <el-button slot=\"reference\" type=\"warning\" icon=\"el-icon-plus\" circle>\r\n                                                </el-button>\r\n                                            </el-popover>\r\n\r\n                                        </el-col>\r\n                                    </el-row>\r\n                                </div>\r\n                            </el-col>\r\n                        </el-row>\r\n                    </div>\r\n\r\n\r\n                    <!-- 分页器 -->\r\n                    <div class=\"dweb\" style=\"margin-top: 7px;\">\r\n                        <el-pagination background layout=\"prev, pager, next\" :total=\"total\" :page-size=\"pageSize\"\r\n                            @current-change=\"currentChange\">\r\n                        </el-pagination>\r\n                    </div>\r\n                </el-col>\r\n            </el-row>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\n    import BreadMenu from '../components/BreadMenu'\r\n    import axios from 'axios'\r\n    import Qs from 'qs'\r\n    export default {\r\n        data() {\r\n            return {\r\n                currentPage: 1, //初始在那页\r\n                currentLanmu: 'nobelong',\r\n                total: 100, //一共多少页\r\n                pageSize: 4, //一页多少条\r\n                article_list: [],\r\n                choosed_lanmu_name: [],\r\n                choose_lanmu_name: \"\",\r\n                new_lanmu_name: '',\r\n                // 栏目结构\r\n                lanmu_tree: [],\r\n                maxid: 0,\r\n                // 文章栏目分配\r\n                choosed_lanmu_id: 0,\r\n                choosed_article_id: 0\r\n\r\n            }\r\n        },\r\n        components: {\r\n            BreadMenu\r\n        },\r\n        mounted() {\r\n            this.getListData(this.currentPage, this.currentLanmu)\r\n            this.getLanmuTree()\r\n        },\r\n        methods: {\r\n            // 选择栏目查看文章\r\n            choosed_lanmu_articleList(node) {\r\n                console.log(node)\r\n                let lanmu_name = node.label\r\n                this.choose_lanmu_name = node.label\r\n                this.getListData(1, lanmu_name)\r\n\r\n            },\r\n            // 选择文章保存栏目\r\n            choosed_lanmu(node) {\r\n                this.choosed_lanmu_id = node.id\r\n                this.choosed_lanmu_name = node.label\r\n\r\n            },\r\n            saveArticleToLanmu(article_id) {\r\n                axios({\r\n                    method: 'put',\r\n                    url: \"http://8.131.225.131:81/api/add-article/\",\r\n                    data: Qs.stringify({\r\n                        token: this.$store.getters.isnotUserlogin,\r\n                        lanmu_id: this.choosed_lanmu_id,\r\n                        article_id: article_id\r\n                    })\r\n                }).then((res) => {\r\n                    console.log(res.data)\r\n                    if (res.data == 'nologin') {\r\n                        alert('尚未登录')\r\n                        return\r\n                    }\r\n                    if (res.data == 'noperm') {\r\n                        alert('权限不足')\r\n                        return\r\n                    }\r\n                    if (res.data == 'ok') {\r\n                        this.getListData(1, this.choosed_lanmu_name);\r\n                        this.getLanmuTree()\r\n                    }\r\n                })\r\n            },\r\n            // 获取栏目数据\r\n            getLanmuTree() {\r\n                axios({\r\n                    method: 'get',\r\n                    url: \"http://8.131.225.131:81/api/dweb-lanmu/\",\r\n                }).then((res) => {\r\n                    console.log(res.data)\r\n                    this.lanmu_tree = res.data\r\n\r\n                })\r\n            },\r\n            // 保存栏目结构\r\n            saveLanmuTree() {\r\n                console.log(this.lanmu_tree)\r\n                axios({\r\n                    method: 'put',\r\n                    url: \"http://8.131.225.131:81/api/dweb-lanmu/\",\r\n                    data: Qs.stringify({\r\n                        token: this.$store.getters.isnotUserlogin,\r\n                        lanmu_tree: JSON.stringify(this.lanmu_tree)\r\n                    })\r\n                }).then((res) => {\r\n                    console.log(res.data)\r\n                    if (res.data == 'nologin') {\r\n                        alert('尚未登录')\r\n                        return\r\n                    }\r\n                    if (res.data == 'noperm') {\r\n                        alert('权限不足')\r\n                        return\r\n                    }\r\n                    if (res.data == 'ok') {\r\n                        this.getLanmuTree()\r\n                    }\r\n\r\n                })\r\n            },\r\n            // 新栏目名称\r\n            pushLanmuList() {\r\n\r\n                let checkTree = this.loopCheckData(this.lanmu_tree)\r\n                console.log(checkTree)\r\n                if (checkTree == false) {\r\n                    this.new_lanmu_name = ''\r\n                    return\r\n                }\r\n                let new_lanmu = {\r\n                    id: this.maxid + 1,\r\n                    label: this.new_lanmu_name,\r\n                    children: []\r\n                }\r\n                this.lanmu_tree.push(new_lanmu)\r\n                this.new_lanmu_name = \"\"\r\n                console.log(new_lanmu)\r\n            },\r\n            // 循环检查数据\r\n            loopCheckData(tree) {\r\n                let checkTree = true\r\n                // 检查新栏目数据\r\n                tree.forEach((obj) => {\r\n                    if (obj.id > this.maxid) {\r\n                        this.maxid = obj.id\r\n                    }\r\n                    if (obj.label == this.new_lanmu_name) {\r\n                        alert('命名重复')\r\n                        checkTree = false\r\n                        return\r\n                    }\r\n                    if (obj.children) {\r\n                        if (obj.children.length > 0) {\r\n                            this.loopCheckData(obj.children)\r\n                        }\r\n                    }\r\n\r\n                });\r\n                return checkTree;\r\n            },\r\n\r\n            getListData(page, lanmu) {\r\n                axios({\r\n                    method: 'get',\r\n                    url: 'http://8.131.225.131:81/api/article-list/',\r\n                    params: {\r\n                        page,\r\n                        pageSize: this.pageSize,\r\n                        lanmu: lanmu\r\n                    },\r\n                }).then((res) => {\r\n                    // console.log(res.data)\r\n                    this.article_list = res.data.data\r\n                    this.total = res.data.total\r\n\r\n                })\r\n            },\r\n\r\n            currentChange(vol) {\r\n                // console.log('第' + vol + '页')\r\n                this.currentPage = vol\r\n                this.getListData(vol)\r\n            },\r\n            remove(_, data) {\r\n                console.log(data.id)\r\n                axios({\r\n                    method: 'delete',\r\n                    headers: {\r\n                        \"Content-Type\": \"application/x-www-form-urlencoded\"\r\n                    },\r\n                    url: \"http://8.131.225.131:81/api/dweb-lanmu/\",\r\n                    data: Qs.stringify({\r\n                        token: this.$store.getters.isnotUserlogin,\r\n                        id: data.id,\r\n                    })\r\n                }).then((res) => {\r\n                    console.log(res.data)\r\n                    if (res.data == 'nologin') {\r\n                        alert('尚未登录')\r\n                        return\r\n                    }\r\n                    if (res.data == 'noperm') {\r\n                        alert('权限不足')\r\n                        return\r\n                    }\r\n                    if (res.data == 'ok') {\r\n                        this.getLanmuTree()\r\n                    }\r\n                })\r\n            },\r\n            renderContent(h, { node, data }) {\r\n                return (\r\n                    <span class=\"custom-tree-node\">\r\n\r\n                        <span>{node.label}</span>\r\n                        <span>({node.data.article_num})</span>\r\n                        <span>\r\n                            <el-button size=\"mini\" type=\"text\" style=\"margin-left:20px\" on-click={() => this.remove(node, data)}>删除</el-button>\r\n                        </span>\r\n                    </span>);\r\n            }\r\n        }\r\n    };\r\n</script>\r\n<style scoped>\r\n    .dweb {\r\n        padding: 10px 10px;\r\n    }\r\n\r\n    .card.dweb .text-item {\r\n        color: #fff;\r\n        height: 80px;\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: center;\r\n    }\r\n\r\n    .text-item span {\r\n        height: 40px;\r\n        line-height: 20px;\r\n        text-overflow: ellipsis;\r\n        overflow: hidden;\r\n    }\r\n</style>"],"sourceRoot":"src/views"}]}